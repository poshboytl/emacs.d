(defun color-theme-blackboard ()
  "Color theme by JD Huntington, based off the TextMate Blackboard theme, created 2008-11-27.
  I revise it a little, just change font-lock-function-name-face to make the headings in org-mode different."
  (interactive)
  (when (fboundp 'color-theme-install)
    (color-theme-install
     '(color-theme-blackboard
       ((background-color . "#0C1021")
        (background-mode . dark)
        (border-color . "black")
        (cursor-color . "#A7A7A7")
        (foreground-color . "#F8F8F8")
        (mouse-color . "sienna1"))
       (default ((t (:background "#0C1021" :foreground "#F8F8F8"))))
       (blue ((t (:foreground "RoyalBlue1"))))
       (bold ((t (:bold t))))
       (bold-italic ((t (:bold t))))
       (border-glyph ((t (nil))))
       (buffers-tab ((t (:background "#0C1021" :foreground "#F8F8F8"))))
       (font-lock-builtin-face ((t (:foreground "#F8F8F8"))))
       (font-lock-comment-face ((t (:italic t :foreground "#AEAEAE"))))
       (font-lock-constant-face ((t (:foreground "#D8FA3C"))))
       (font-lock-doc-string-face ((t (:foreground "DarkOrange"))))
       (font-lock-function-name-face ((t (:foreground "#CD5C5C")))) ; original is #FF6400
       (font-lock-keyword-face ((t (:foreground "#FBDE2D"))))
       (font-lock-preprocessor-face ((t (:foreground "Aquamarine"))))
       (font-lock-reference-face ((t (:foreground "SlateBlue"))))
       
       (font-lock-regexp-grouping-backslash ((t (:foreground "#E9C062"))))
       (font-lock-regexp-grouping-construct ((t (:foreground "red"))))
       
       (font-lock-string-face ((t (:foreground "#61CE3C"))))
       (font-lock-type-face ((t (:foreground "#8DA6CE"))))
       (font-lock-variable-name-face ((t (:foreground "#FF6400"))))
       (font-lock-warning-face ((t (:bold t :foreground "Pink"))))
       (gui-element ((t (:background "#D4D0C8" :foreground "black"))))
       (region ((t (:background "#253B76"))))
       (mode-line ((t (:background "gray75" :foreground "black"))))
       (highlight ((t (:background "gray21")))) ; original is #222222
       (highline-face ((t (:background "gray21"))))
       (italic ((t (nil))))
       (left-margin ((t (nil))))
       (text-cursor ((t (:background "yellow" :foreground "black"))))
       (toolbar ((t (nil))))
       (underline ((nil (:underline nil))))
       (zmacs-region ((t (:background "snow" :foreground "blue"))))

       ;; -- more customized faces
       (anything-dir-priv ((t (:background "DarkBlue" :foreground "#CD5C5C"))))
       (anything-file-name ((t (:foreground "RoyalBlue1"))))
       (anything-header ((t (:background "#253B76" :foreground "#FBDE2D"))))
       (anything-isearch-match ((t (:inherit isearch))))
       (diff-added ((t (:foreground "#559944"))))
       (diff-context ((t (:foreground "grey60"))))
       (diff-file-header ((((class color) (min-colors 88) (background dark)) (:foreground "RoyalBlue1"))))
       (diff-function ((t (:foreground "#00bbdd"))))
       (diff-header ((((class color) (min-colors 88) (background dark)) (:foreground "RoyalBlue1"))))
       (diff-hunk-header ((t (:foreground "#fbde2d"))))
       (diff-nonexistent ((t (:inherit diff-file-header :strike-through nil))))
       (diff-refine-change ((((class color) (min-colors 88) (background dark)) (:background "#182042"))))
       (diff-removed ((t (:foreground "#de1923"))))
       (diredp-deletion ((t (:foreground "Red"))))
       (diredp-dir-heading ((t (:background "#253B76" :foreground "#FBDE2D"))))
       (diredp-dir-priv ((t (:background "DarkBlue" :foreground "#CD5C5C"))))
       (diredp-display-msg ((t (:foreground "#253B76"))))
       (diredp-exec-priv ((t (:foreground "green"))))
       (diredp-file-name ((t (:foreground "RoyalBlue1"))))
       (diredp-flag-mark ((t (:foreground "Yellow"))))
       (diredp-flag-mark-line ((t (:inherit font-lock-warning-face))))
       (diredp-no-priv ((t (:foreground "gray20"))))
       (diredp-other-priv ((t (:foreground "PaleGoldenrod"))))
       (diredp-rare-priv ((t (:foreground "gold"))))
       (diredp-read-priv ((t (:foreground "MediumAquamarine"))))
       (diredp-write-priv ((t (:foreground "Orchid"))))
       (doc-title-1-face ((t (:inherit outline-1))))
       (doc-title-2-face ((t (:inherit outline-2))))
       (doc-title-3-face ((t (:inherit outline-3))))
       (doc-title-4-face ((((class color) (background dark)) (:inherit outline-4))))
       (ecb-default-highlight-face ((((class color) (background dark)) (:background "blue"))))
       (ediff-current-diff-A ((((class color) (min-colors 16)) (:background "#1D4266" :foreground "#DDDDDD"))))
       (ediff-current-diff-B ((((class color) (min-colors 16)) (:background "#1D4266" :foreground "#DDDDDD"))))
       (ediff-even-diff-A ((((class color) (min-colors 16)) (:background "#151B38" :foreground "gray50"))))
       (ediff-even-diff-B ((((class color) (min-colors 16)) (:background "#151B38" :foreground "gray50"))))
       (ediff-fine-diff-A ((((class color) (min-colors 16)) (:background "#557DA3" :foreground "dark blue"))))
       (ediff-fine-diff-B ((((class color) (min-colors 16)) (:background "#506B85" :foreground "green"))))
       (ediff-odd-diff-A ((t (:stipple nil :background "#232738" :foreground "gray50" :slant normal))))
       (ediff-odd-diff-B ((t (:stipple nil :background "#232738" :foreground "gray50" :slant normal))))
       (escape-glyph ((((background dark)) (:foreground "#40434D"))))
       (flymake-errline ((((class color) (background dark)) (:underline "Firebrick1"))))
       (header-line ((nil (:background "#253B76" :underline "gray60" :width condensed))))
       (hl-line ((t (:inherit highline-face))))
       (hl-paren-face ((t (:weight bold))) t)
       (ido-first-match ((t (:foreground "ForestGreen" :weight bold))))
       (ido-only-match ((((class color)) (:background "#253B76" :foreground "#F8F8F8" :weight bold))))
       (ido-subdir ((((min-colors 88) (class color)) (:foreground "cyan"))))
       (info-elisp-command-ref-item ((t (:foreground "#FBDE2D"))))
       (info-elisp-function-ref-item ((t (:foreground "#CD5C5C"))))
       (info-elisp-macro-ref-item ((t (:foreground "#AEAEAE"))))
       (info-elisp-reference-item ((t nil)))
       (info-elisp-special-form-ref-item ((t (:foreground "LightSalmon"))))
       (info-elisp-syntax-class-item ((t (:foreground "#8DA6CE"))))
       (info-elisp-user-option-ref-item ((t (:foreground "Red"))))
       (info-elisp-variable-ref-item ((t (:foreground "#FF6400"))))
       (info-file ((t (:foreground "RoyalBlue1"))))
       (info-quoted-name ((t (:foreground "#D8FA3C"))))
       (info-string ((t (:inherit font-lock-string-face :foreground "#61CE3C"))))
       (link ((((class color) (min-colors 88) (background dark)) (:foreground "cyan1" :underline nil))))
       (magit-branch ((t (:inherit magit-header :foreground "tomato" :weight bold))))
       (magit-diff-add ((((class color) (background dark)) (:inherit diff-added))))
       (magit-diff-del ((((class color) (background dark)) (:inherit diff-removed))))
       (magit-diff-file-header ((t (:inherit diff-file-header))))
       (magit-diff-hunk-header ((t (:inherit diff-hunk-header))))
       (magit-diff-none ((t (:inherit diff-context))))
       (magit-item-highlight ((((class color) (background dark)) (:background "#141433"))))
       (magit-section-title ((t (:inherit outline-1))))
       (markdown-inline-code-face ((t (:inherit font-lock-constant-face))))
       (markdown-pre-face ((default nil) (nil nil)))
       (mumamo-background-chunk-submode1 ((((class color) (min-colors 88) (background dark)) (:background "#192042"))))
       (mumamo-background-chunk-submode2 ((((class color) (min-colors 88) (background dark)) (:background "#192042"))))
       (mumamo-background-chunk-submode3 ((((class color) (min-colors 88) (background dark)) (:background "#192042"))))
       (mumamo-background-chunk-submode4 ((((class color) (min-colors 88) (background dark)) (:background "#192042"))))
       (pulse-highlight-start-face ((((class color) (background dark)) (:background "blue"))))
       (sml-modeline-end-face ((t (:background "black" :foreground "white"))))
       (sml-modeline-vis-face ((t (:inherit region :foreground "white"))))
       (whitespace-hspace ((((class color) (background dark)) (:background "#1C2033" :foreground "aquamarine3"))))
       (whitespace-indentation ((t (:background "#B0A741" :foreground "firebrick"))))
       (whitespace-line ((t (:background "gray12" :foreground "violet"))))
       (whitespace-newline ((t (:foreground "#40434D" :underline nil :weight normal))))
       (whitespace-space ((((class color) (background dark)) (:background "#1C2033" :foreground "aquamarine3"))))
       (whitespace-space-after-tab ((t (:background "#B0A741" :foreground "firebrick"))))
       (whitespace-space-before-tab ((t (:background "#C49548" :foreground "firebrick"))))
       (whitespace-tab ((((class color) (background dark)) (:background "#1C2033" :foreground "aquamarine3"))))
       (whitespace-trailing ((t (:background "#40434D" :foreground "#40434D" :weight normal))))
       (yaoddmuse-url ((t (:foreground "Cyan" :underline t))))
       (zencoding-preview-input ((t (:box (:line-width 1 :color "#1D4266")))))
       (zencoding-preview-output ((nil (:background "#151B38"))))))))
